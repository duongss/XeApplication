<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextFeature1">
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>

    <style name="TextFeature2">
        <item name="android:textColor">@color/color8</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="TextFeature3">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/svn_diablo</item>
    </style>

    <style name="TextFeature4">
        <item name="android:textColor">@color/color2</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="TextFeature5">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/neuton_light</item>
    </style>

    <style name="CustomThemeOverlay_MaterialCalendar_Fullscreen" parent="@style/ThemeOverlay.MaterialComponents.MaterialCalendar.Fullscreen">
        <item name="materialCalendarStyle">@style/Custom_MaterialCalendar.Fullscreen</item>
    </style>

    <style name="Custom_MaterialCalendar.Fullscreen" parent="@style/Widget.MaterialComponents.MaterialCalendar.Fullscreen">
        <item name="android:windowFullscreen">false</item>
    </style>
    <style name="RelativeLayoutStyle14" parent="@android:style/Widget">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:padding">14dp</item>
    </style>
</resources>